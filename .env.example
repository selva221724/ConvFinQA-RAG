# OpenAI API Key (Required)
OPENAI_API_KEY=your_openai_api_key_here

# Pinecone API Key (Required for Pinecone vector storage)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=us-east-1  # AWS region for serverless Pinecone
PINECONE_INDEX=convfinqa  # Your Pinecone index name

# Optional: Uncomment to set a specific model
# OPENAI_MODEL=gpt-3.5-turbo  # Options: gpt-3.5-turbo, gpt-4, etc.
